<mvc:View controllerName="iot.timetracking-projects-ui.controller.Object" 
	xmlns="sap.m" 
	xmlns:mvc="sap.ui.core.mvc">

	<Page title="{title}" busy="{objectView>/busy}" busyIndicatorDelay="{objectView>/delay}">
		<content>
			<Table id="table" mode="MultiSelect" growing="true" growingScrollToLoad="true">

				<columns>
					<Column>
						<Text text="Name"/>
					</Column>
				</columns>

			</Table>
		</content>

	</Page>

</mvc:View>